# 12-Factor Agents Configuration

# Application Settings
APP_NAME=12-factor-agents
DEBUG=false

# API Configuration  
API_HOST=0.0.0.0
API_PORT=8000

# Database Configuration
DATABASE_URL=postgresql+asyncpg://user:password@localhost/12factor
REDIS_URL=redis://localhost:6379/0

# LLM Provider API Keys (set at least one)
OPENAI_API_KEY=your_openai_api_key_here
ANTHROPIC_API_KEY=your_anthropic_api_key_here

# HumanLayer Configuration
HUMANLAYER_API_KEY=your_humanlayer_api_key_here
HUMANLAYER_VERBOSE=true

# Context Management
MAX_CONTEXT_TOKENS=100000
CONTEXT_COMPRESSION_THRESHOLD=0.8

# Logging
LOG_LEVEL=INFO
STRUCTURED_LOGGING=true